{
  "name": "jeyroik/extas-installer",
  "description": "Extas installer package",
  "require": {
    "symfony/finder": "5.*",
    "symfony/console": "5.*",
    "jeyroik/extas-foundation": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vlucas/phpdotenv": "^3.4",
    "alcaeus/mongo-php-adapter": "1.*",
    "jeyroik/extas-repositories-mongo": "0.*"
  },
  "autoload": {
    "psr-4": {
      "extas\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox tests"
  },
  "bin": [
    "src/bin/extas"
  ]
}